    <appwidget
        launcher:packageName="com.android.settings"
        launcher:className="com.android.settings.widget.SettingsAppWidgetProvider"
        launcher:screen="1"
        launcher:x="2"
        launcher:y="1"
        launcher:spanX="4"
        launcher:spanY="1" />

    <!-- Middle screen [2] -->

    <!-- Right screen [3] -->

